{
    "manifest_version": 2,
    "name": "Arxiv Downloader",
    "version": "1.0",
    "description" : "This is a chrome extension for downloading arXiv papers",
    "permissions" : ["tabs", "downloads"],
    "commands": {
        "EventName": {
            "suggested_key": {
                "default": "Ctrl+Shift+A"
            },
            "description": "description of this event"
        }
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action" : {
        "default_icon" : "images/icon_19.png",
        "default_title" : "Popup",
        "default_popup" : "popup.html"
     }
}
